{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "es2022",
    "rootDir": ".",
    "moduleResolution": "bundler",
    "baseUrl": "./",
    "sourceMap": true,
    "outDir": "./lib",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "newLine": "lf"
  },
  "exclude": [
    "node_modules",
  ],
    "include": [
        "__tests__/**/*.ts"
    ],
}
